pipeline {
    options {
        buildDiscrder (logRotator(numTokeepstr: '5', artifactNumTokeepstr: '5'))
    }
    agent any

    tools {
        maven 'maven_3.9.4'
    }

    stage {
        stage('code Compilation') {
            steps {
                echo 'code compilation is in progress!'
                sh 'mvn clean compile'
                echo 'code compile is complete successfully'
            }
        }
    }
}

